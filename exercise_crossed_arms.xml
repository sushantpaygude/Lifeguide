<?xml version="1.0" encoding="UTF-8"?>
<assessmentItem
  xmlns="http://www.imsglobal.org/xsd/imsqti_v2p1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.imsglobal.org/xsd/imsqti_v2p1 imsqti_v2p1.xsd"
  identifier="exercise_crossed_arms"
  title="exercise_crossed_arms"
  adaptive="false"
  timeDependent="false">
  <stylesheet href="lifecit_logo_aqua.css" type="text/css"/>
  <stylesheet href="exercise_crossed_arms-out.css" type="text/css"/>
  <itemBody>
    <div id="container-body">
      <div>
        <img id="image-1_lifecit_logo_aqua" src="background4.png" alt="White background"/>
      </div>
      <div>
        <img id="image-2_lifecit_logo_aqua" src="aqua_rectangle.jpg" alt="Aqua rectangle"/>
      </div>
      <div>
        <img id="image-5_lifecit_logo_aqua" src="mitt_small.png" alt="Mitt"/>
      </div>
      <div id="textbox-1_lifecit_logo_aqua" class="includeBorderAndPadding">
        <span class="textbox-1-style1_lifecit_logo_aqua">
          STRONG
        </span>
        <br/>
      </div>
      &#160;
      <div>
        <img id="image-1" src="exercise_crossed_arms_ab.gif" alt="change me"/>
      </div>
      <div id="button-1" class="submit-jumpto-button" label="exercise_cane_raise">
        NEXT
      </div>
      <div id="textbox-1" class="includeBorderAndPadding">
        <span class="textbox-1-style1">
          Arm Opening
        </span>
      </div>
      <div id="textbox-2" class="includeBorderAndPadding">
        Start with your arms crossed on your body, with your hands at the level of your shoulders
      </div>
      <div id="textbox-3" class="includeBorderAndPadding">
        Open both your arms, moving at the same time, like a bird opening their wings
      </div>
      <div id="textbox-4" class="includeBorderAndPadding">
        Hold for 5 seconds 
      </div>
      <div id="textbox-5" class="includeBorderAndPadding">
        Repeat 5 times
      </div>
      <div id="button-2" class="submit-button">
        Back
      </div>
    </div>
  </itemBody>
</assessmentItem>